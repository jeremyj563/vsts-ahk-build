{
    "manifestVersion": 1,
    "id": "ahk-build",
    "publisher": "JeremyJohnson",
    "version": "1.1.0",
    "name": "AHK Build",
    "description": "Compile AutoHotKey scripts using Ahk2Exe.",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "ahk-build-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "AHK Build Hub",
                "uri": "hub.html"
            }
        }
    ],
    "files": [
        {
            "path": "hub.html",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "ahk-build.ps1",
            "addressable": true
        }
    ]
}
